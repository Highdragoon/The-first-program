import java.util.ArrayList;
public class Inventory {

	private ArrayList<Item> items;
	
	public Inventory() {
		items= new ArrayList<>();
	}
	
	public void additem(Item Item) {
		items.add(Item);
	}
	
	public void displayInventory() {
		for (Item Item : items) {
			System.out.println("Item: " + Item.getName() + ", quantity: " + Item.getQuantity());
		}
	}
	
}
